{
  "name": "angular-state-adapt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --no-cache --runInBand",
    "test:ci": "npx jest --ci --runInBand",
    "test:watch": "jest --no-cache --watch",
    "test:update-snapshots": "npx jest --runInBand --updateSnapshot",
    "report:combined": "./scripts/report.sh",
    "lint": "ng lint",
    "prepare": "husky",
    "e2e": "ng e2e",
    "e2e:interactive": "npx playwright test --ui",
    "e2e:chrome": "npx playwright test --project=chromium",
    "e2e:debug": "npx playwright test --debug",
    "e2e:codegen": "npx playwright codegen",
    "e2e:report": "npx playwright show-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@jsverse/transloco": "^7.5.1",
    "@state-adapt/angular": "^2.0.8",
    "@state-adapt/core": "^2.0.8",
    "@state-adapt/rxjs": "^2.0.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@playwright/test": "^1.50.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.1",
    "nyc": "^17.1.0",
    "@ngx-playwright/test": "^0.5.11",
    "playwright": "^1.50.1",
    "playwright-ng-schematics": "^2.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.18.0"
  }
}
